<div class="row">
  <div class="col s12 m5">
    <div class="card">
      <div class="card-content">
        <span class="card-title">Currently Working On</span>
        <ul>
          <% @processing_jobs.each do |job| %>
            <h5><%= link_to job.title, job_path(job) %></h5>
            <hr>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
  <div class="col s12 m5">
    <div class="card">
      <div class="card-content">
        <span class="card-title">Your Other Claimed Jobs</span>
        <ul>
          <% @claimed_jobs.each do |job| %>
            <h5><%= link_to job.title, job_path(job) %></h5>
          <hr>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col s12 m5">
    <div class="card">
      <div class="card-content">
        <span class="card-title">Completed Jobs</span>
        <ul>
          <% @completed_jobs.each do |job| %>
            <h5><%= link_to job.title, job_path(job) %></h5>
            <hr>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
